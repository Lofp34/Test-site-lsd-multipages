# Local development template (no secrets)

# Base URL used across utilities and tests
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Disable background jobs/monitoring locally and in CI preview builds
AUDIT_SCHEDULE_ENABLED=false
VERCEL_MONITORING_ENABLED=false

# Chat (disabled by default without keys)
NEXT_PUBLIC_CHAT_ENABLED=false
NEXT_PUBLIC_GEMINI_API_KEY=

# Email (left blank to avoid accidental sends)
SENDGRID_API_KEY=
SENDGRID_FROM_EMAIL=noreply@example.com
SENDGRID_FROM_NAME=Dev Local
ADMIN_EMAIL=dev@example.com

# Supabase (not required for local browsing)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Vercel integration (not needed locally)
VERCEL_API_TOKEN=
VERCEL_PROJECT_ID=
VERCEL_TEAM_ID=
